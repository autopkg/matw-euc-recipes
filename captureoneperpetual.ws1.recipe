	<key>Identifier</key>
	<string>com.github.matweuc.ws1.captureoneperpetual</string>
	<key>ParentRecipe</key>
	<string>com.github.foigus.munki.CaptureOne</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>Capture One</string>
		<key>MUNKI_REPO_SUBDIR</key>
		<string>apps/%NAME%</string>
		<key>MUNKI_CATEGORY</key>
		<string>Productivity</string>
		<key>WS1_OAUTH_CLIENT_ID</key>
		<string>OAUTH2_CLIENT_ID_HERE</string>
		<key>WS1_OAUTH_CLIENT_SECRET</key>
		<string>OAUTH2_CLIENT_CLIENT_SECRET_HERE</string>
		<key>WS1_OAUTH_TOKEN_URL</key>
		<string>OAUTH2_ACCESS_TOKEN_SERVER_URL_HERE</string>
		<key>WS1_API_USERNAME</key>
		<string>API_USERNAME_HERE</string>
		<key>WS1_API_PASSWORD</key>
		<string>API_PASSWORD_HERE</string>
		<key>WS1_API_TOKEN</key>
		<string>nn3kmW6SImGvaEPUqjGukHkk51LBFt5AIl4c4r3JJ+c=</string>
		<key>WS1_API_URL</key>
		<string>https://as1688.awmdm.com</string>
		<key>WS1_API_USERNAME</key>
		<string>API_USERNAME_HERE</string>
		<key>WS1_B64ENCODED_API_CREDENTIALS</key>
		<string>Basic bWF0d1xzdmNfd3MxREVQOiRtUTgkdCYxWEUkalRiaGM=</string>
		<key>WS1_CONSOLE_URL</key>
		<string>https://cn1688.awmdm.com</string>
		<key>WS1_GROUPID</key>
		<string>matwglobal</string>
		<key>WS1_OAUTH_CLIENT_ID</key>
		<string>OAUTH2_CLIENT_ID_HERE</string>
		<key>WS1_OAUTH_CLIENT_SECRET</key>
		<string>OAUTH2_CLIENT_CLIENT_SECRET_HERE</string>
		<key>WS1_OAUTH_TOKEN_URL</key>
		<string>OAUTH2_ACCESS_TOKEN_SERVER_URL_HERE</string>
